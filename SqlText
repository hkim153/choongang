      M_code VARCHAR2(10 BYTE) DEFAULT '00', 
      S_code VARCHAR2(10 BYTE) DEFAULT '00', 
      B_Name VARCHAR2(100 BYTE),
      M_Name VARCHAR2(100 BYTE),
      S_Name VARCHAR2(100 BYTE)
    );
    
    
