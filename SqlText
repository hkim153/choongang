---------------- 현호 ---------------------
CREATE TABLE EVENT
   (	E_ID NUMBER NOT NULL ENABLE, 
	TITLE VARCHAR2(100 BYTE), 
	DESCRIPTION VARCHAR2(150 BYTE), 
	E_START VARCHAR2(20 BYTE), 
	E_END VARCHAR2(20 BYTE), 
	E_TYPE VARCHAR2(20 BYTE), 
	TEXTCOLOR VARCHAR2(20 BYTE), 
	ALLDAY VARCHAR2(20 BYTE), 
	BACKGRUNDCOLOR VARCHAR2(20 BYTE) , 
	RSA VARCHAR2(50 BYTE), 
	URL VARCHAR2(200 BYTE), 
  CONSTRAINT "EVENT_PK" PRIMARY KEY ("E_ID"));
  

